			"metadata": "Data Factory name",
			"defaultValue": "balajichollangi"
		},
		"ls_adlstoadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlstoadls'"
		},
		"ls_azsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azsql'"
		},
		"ls_adlstoadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://balajich.dfs.core.windows.net/"
